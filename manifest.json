{
  "manifest_version": 3,
  "name": "Sizr",
  "description": "Size tool",
  "version": "0.1",
  "action": {
    "default_popup": "./popup/popup.html",
    "default_icon": "./images/icon.png"
  },
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": [
        "https://google.ca/*"
      ]
    }
  ]
}
